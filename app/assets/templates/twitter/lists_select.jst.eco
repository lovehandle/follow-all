<label id="follow-all-twitter-list-label" for="follow-all-twitter-list-input" class="control-label">List:</label>
<div class="controls">
  <select id="follow-all-twitter-list-input" name="twitter[list]">
    <option></option>
    <% @lists.each (list) -> : %>
      <option value="<%= list.get("name") %>"><%= list.get("name") %></option>
    <% end %>
  </select>
</div>
